#! /usr/bin/bash
alias jupy='jupyter notebook \
  --NotebookApp.allow_origin='https://colab.research.google.com' \
  --port=8888 \
  --NotebookApp.port_retries=0'

alias nv='nvim' \
    pxy='export https_proxy="http://127.0.0.1:8118" && export http_proxy="http://127.0.0.1:8118"' \
    ec='emacsclient -t' \
    em='emacs --with-profile emacs &' \
    jn='jupyter notebook' \
    ra='ranger' \
    nb='pxy && newsboat' \
	yt="pxy && youtube-dl --add-metadata -i -o '%(upload_date)s-%(title)s.%(ext)s'" \
	yta='pxy && yt -x -f bestaudio/best' \
    ytmp3='pxy && youtube-dl --extract-audio --audio-format mp3' \
    ytdl='pxy && youtube-dl' \
    spi='sudo pacman -S' \
    spu='sudo pacman -Syu' \
    mpd='pxy && mpd'
